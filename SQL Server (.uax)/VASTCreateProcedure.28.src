CREATE PROCEDURE [dbo].[GetArticleForDropdownlist]

AS
BEGIN

	SET NOCOUNT ON;
  
	SELECT CAT_ID, CAT_NAME From Cooking_Article_Category WITH (NOLOCK)

END

		