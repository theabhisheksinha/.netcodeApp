CREATE TABLE dbo.Events (
  EVENT_ID int NOT NULL,
  DATE_ADDED datetime NOT NULL,
  START_DATE datetime NOT NULL,
  CATEGORY varchar NULL,
  END_DATE datetime NOT NULL,
  EVENT_TITLE nvarchar NOT NULL,
  EVENT_DETAILS nvarchar NULL,
  APPMEETING_STARTTIME varchar NULL,
  APPMEETING_ENDTIME varchar NULL,
  UID int NOT NULL,
  PRIVATE int NOT NULL,
  PRIMARY KEY CLUSTERED ( EVENT_ID ASC  ) WITH (IGNORE_DUP_KEY = OFF) ON [PRIMARY]
)
