CREATE PROCEDURE UpdateArticleCategory

@Catid int,
@CatName varchar(100)

AS
BEGIN

	SET NOCOUNT ON;
    
    Update Cooking_Article_Category Set CAT_NAME = @CatName Where CAT_ID = @Catid

 
END

		